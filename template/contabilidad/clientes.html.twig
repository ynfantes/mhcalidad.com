{% endblock %}
{% block contenido %}
<!-- MAIN CONTENT AREA -->
<div class="main-content">
<div class="container">
    <div class="row show-grid">
        <div class="col-md-12 col-sm-12">
            <!-- BREADCRUMBS -->
            <div id="breadcrumb">
                <ul>
                    <li class="home"><a href="{{constant('ROOT')}}contabilidad/">MH Calidad Administraci√≥n y Contabilidad</a></li>
                    <li>{{session.usuario.nombre}}</li>
